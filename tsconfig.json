{
	"compilerOptions": {
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"declaration": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"suppressImplicitAnyIndexErrors": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"importHelpers": true,
		"noEmitHelpers": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"pretty": true,
		"target": "es2015",
		"jsx": "react",
		"allowJs": true
	},
	"formatCodeOptions": {
		"indentSize": 4,
		"tabSize": 4
	}
}
